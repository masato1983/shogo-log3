{
    "name": "portfolio",
    "version": "1.0.0",
    "description": "portfolio",
    "main": "index.js",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "npm:check": "ncu",
        "npm:update": "ncu -u",
        "start:server": "browser-sync start -s dist --https true --ghostMode false -w src/*.html src/assets/css/*.css src/assets/js/*.js",
        "compile:ejs": "ejs-cli -b src/ejs/ \"/*.ejs\" -o dist/ -O ./src/ejs/meta/meta.json",
        "postcompile:ejs": "run-s format:html lint:html",
        "compile:postcss": "postcss dist/assets/css/style.css --config postcss.config.js -o dist/assets/css/style.css",
        "compile:sass": "sass --error-css --charset src/assets/scss/style.scss dist/assets/css/style.css",
        "compile:sass2css": "run-s compile:sass compile:postcss",
        "postcompile:sass2css": "run-s format:css lint:css",
        "compile:js:dev": "webpack --mode=development",
        "compile:js:prod": "webpack --mode=production",
        "postcompile:js:dev": "npm run lint:js",
        "postcompile:js:prod": "npm run lint:js",
        "compile:all:dev": "run-p compile:ejs compile:sass2css compile:js:dev",
        "compile:all:prod": "run-p compile:ejs compile:sass2css compile:js:prod",
        "watch:ejs": "chokidar \"src/ejs/\" -c \"npm run compile:ejs\" --initial",
        "watch:sass2css": "chokidar \"src/assets/scss/\" --command \"npm run compile:sass2css\" --initial",
        "watch:js": "chokidar \"src/assets/js/**/*\" -c \"npm run compile:js:dev\" --initial",
        "watch:img": "chokidar \"src/assets/img/**/*\" --command \"npm run copy:img\" --initial",
        "watch:all": "cross-env NODE_ENV=local run-p watch:ejs watch:sass2css watch:js watch:img start:server",
        "prewatch:all": "run-s delete:dist copy:css copy:font copy:favicon copy:rfg copy:maskable copy:webclip copy:robots",
        "copy:css": "cpx \"src/assets/css/vendors/*.css\" dist/assets/css/",
        "copy:font": "cpx \"src/assets/font/output/**/*\" dist/assets/font/",
        "copy:img": "cpx \"src/assets/img/**/*\" --ignore \"src/assets/img/favicon/\",\"src/assets/img/webclip\" dist/assets/img/",
        "copy:favicon": "cpx \"src/assets/img/favicon/*\" dist/",
        "copy:rfg": "cpx \"src/assets/img/favicon/rfg/**/*\" --ignore \"src/assets/img/favicon/rfg/html_code.html\",\"src/assets/img/favicon/rfg/README.md\" dist/",
        "copy:maskable": "cpx \"src/assets/img/favicon/maskable/**/*\" dist/",
        "copy:webclip": "cpx \"src/assets/img/webclip/**/*\" dist/",
        "copy:robots": "cpx \"src/robots.txt\" dist/",
        "delete:dist": "rimraf dist",
        "delete:map": "rimraf dist/**/*.map",
        "lint:html": "linthtml \"dist/**/*.html\"",
        "lint:css": "stylelint \"dist/assets/css/*.css\" --config .stylelintrc.json --formatter verbose",
        "lint:scss": "stylelint \"**/*.scss\" --config .stylelintrc.json --formatter verbose",
        "lint:js": "eslint \"src/assets/js/**/*.js\"",
        "lint:all": "run-p lint:html lint:css lint:scss lint:js",
        "format:html": "prettier --config .prettierrc.json --write \"dist/**/*.html\"",
        "format:css": "stylelint \"dist/assets/css/*.css\" --config .stylelintrc.json --fix",
        "format:scss": "stylelint \"**/*.scss\" --config .stylelintrc.json --fix",
        "format:js": "eslint \"src/assets/js/**/*.js\" --fix",
        "format:all": "run-p format:html format:css format:js",
        "min:css": "cleancss -o dist/assets/css/style.css dist/assets/css/style.css",
        "min:js": "uglifyjs dist/assets/js/script.js -o dist/assets/js/script.js",
        "min:all": "run-p min:css min:js",
        "zip:css": "gzip dist/**/*.css --extension=gz --extension=br",
        "zip:js": "gzip dist/**/*.js --extension=gz --extension=br",
        "zip:svg": "gzip dist/**/*.svg --extension=gz --extension=br",
        "zip:all": "run-p zip:css zip:js zip:svg",
        "prepare:all": "run-p min:js min:css delete:map",
        "cachebusting": "gulp cacheBusting",
        "subset:unicode": "glyphhanger https://shogo-log2.coding11ty.com",
        "subset:fonttools:otf": "pyftsubset src/assets/font/original/NotoSansJP-Bold.otf --layout-features=* --unicodes-file=src/assets/font/specify/specify-unicode.txt --output-file=src/assets/font/output/NotoSansJP-Bold-subset.otf --verbose",
        "subset:fonttools:woff": "pyftsubset src/assets/font/original/NotoSansJP-Bold.otf --flavor=woff --layout-features=* --unicodes-file=src/assets/font/specify/specify-unicode.txt --output-file=src/assets/font/output/NotoSansJP-Bold-subset.woff --verbose",
        "subset:fonttools:woff2": "pyftsubset src/assets/font/original/NotoSansJP-Bold.otf --flavor=woff2 --layout-features=* --unicodes-file=src/assets/font/specify/specify-unicode.txt --output-file=src/assets/font/output/NotoSansJP-Bold-subset.woff2 --verbose",
        "subset:fonttools:all": "run-p subset:fonttools:otf subset:fonttools:woff subset:fonttools:woff2",
        "build:dev": "cross-env NODE_ENV=development run-s --continue-on-error delete:dist compile:all:dev lint:scss copy:img copy:css cachebusting copy:font zip:all copy:favicon copy:rfg copy:maskable copy:webclip copy:robots",
        "build:prod": "cross-env NODE_ENV=production run-s --continue-on-error delete:dist compile:all:prod lint:scss copy:img copy:css delete:map min:all cachebusting copy:font zip:all copy:favicon copy:rfg copy:maskable copy:webclip copy:robots"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/masato1983/portfolio.git"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/masato1983/portfolio/issues"
    },
    "homepage": "https://github.com/masato1983/portfolio#readme",
    "devDependencies": {
        "@babel/core": "7.17.5",
        "@babel/preset-env": "7.16.11",
        "@googlemaps/js-api-loader": "1.13.7",
        "@linthtml/linthtml": "0.8.4",
        "@linthtml/linthtml-config-recommended": "0.1.0",
        "@namics/stylelint-bem": "7.0.0",
        "@squoosh/cli": "0.7.2",
        "@typescript-eslint/eslint-plugin": "5.12.1",
        "@typescript-eslint/parser": "5.12.1",
        "animejs": "3.2.1",
        "aos": "3.0.0-beta.6",
        "autoprefixer": "10.4.2",
        "babel-loader": "8.2.3",
        "body-scroll-lock": "4.0.0-beta.0",
        "browser-sync": "2.27.7",
        "chokidar-cli": "3.0.0",
        "circletype": "^2.3.0",
        "clean-css-cli": "5.5.2",
        "core-js": "3.21.1",
        "cpx2": "4.2.0",
        "cross-env": "7.0.3",
        "delicious-hamburgers": "1.2.2",
        "destyle.css": "3.0.2",
        "easings-css": "2.1.1",
        "ejs-cli": "2.2.3",
        "eslint": "8.10.0",
        "eslint-config-prettier": "8.4.0",
        "eslint-plugin-prettier": "4.0.0",
        "focus-trap": "6.7.3",
        "focus-visible": "5.2.0",
        "formbouncerjs": "1.4.6",
        "frontline-sass": "3.11.0",
        "gulp": "4.0.2",
        "gulp-if": "3.0.0",
        "gulp-rev-all": "3.0.0",
        "gulp-rev-delete-original": "0.2.3",
        "gzip-cli": "1.2.0",
        "hover-media-query": "1.1.1",
        "ie11-custom-properties": "4.1.0",
        "intersection-observer": "0.12.0",
        "js-cookie": "3.0.1",
        "mkdirp": "1.0.4",
        "nodelist-foreach-polyfill": "1.2.0",
        "npm-check-updates": "12.4.0",
        "npm-run-all": "4.1.5",
        "object-fit-images": "3.2.4",
        "path": "0.12.7",
        "picturefill": "3.0.3",
        "postcss": "8.4.7",
        "postcss-100vh-fix": "1.0.2",
        "postcss-cli": "9.1.0",
        "postcss-clip-path-polyfill": "1.1.0",
        "postcss-easing-gradients": "3.0.1",
        "postcss-flexbugs-fixes": "5.0.2",
        "postcss-gap-properties": "3.0.3",
        "postcss-object-fit-images": "1.1.2",
        "postcss-rem": "2.0.2",
        "postcss-viewport-height-correction": "1.1.1",
        "prettier": "2.5.1",
        "rellax": "1.12.1",
        "rimraf": "3.0.2",
        "sass": "1.49.9",
        "sass-mq": "6.0.0",
        "smooth-scroll": "16.1.3",
        "stylelint": "14.5.3",
        "stylelint-config-rational-order-fix": "0.1.9",
        "stylelint-config-recommended-scss": "5.0.2",
        "stylelint-config-sass-guidelines": "9.0.1",
        "stylelint-config-standard": "24.0.0",
        "stylelint-order": "5.0.0",
        "stylelint-scss": "4.1.0",
        "swiper": "8.0.6",
        "uglify-es": "3.3.9",
        "vivus": "0.4.6",
        "webpack": "5.69.1",
        "webpack-cli": "4.9.2",
        "webpack-dev-server": "4.7.4"
    }
}
